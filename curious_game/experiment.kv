<Config>:
# Nao ip configuration and subject id
    ip_num:ip_num
    orientation:'vertical'
    BoxLayout:
        size_hint_y:0.1
        padding:'5dp'
        spacing:'5dp'
        Label:
            text:'Admin, config your IP!'
            font_size:30
            font_name: 'dogs'
            color:0,0,0,1
    BoxLayout:
        size_hint_y:0.1
        TextInput:
            id:ip_num
            padding_x: self.width/3
            padding_y: ( self.height - self.line_height ) / 2
            text:"Insert IP address"
            font_size:20
            color:0,0,0,1
            on_focus:root.delete_text()
    BoxLayout:
        size_hint_y:0.3
        Button:
            text:"Config"
            color:0,0,0,1
            font_size:30
            bold: True
            on_press: self.background_color = (1,0,0,1)
            on_release: app.btn_released(self, app.goto_welcome)  # function to be called when button is pressed
    BoxLayout:
        size_hint_y:0.6